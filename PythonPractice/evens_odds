def even_odd(x):
    
    if x % 4 == 0:
        print ('Your number is divisible by 4.')
    elif x % 2 == 0:
        print ('Your number is even.')
    else:
        print ('Your number is an odd number.')

def dual_num(x,y):
    
    if y/x % 2 == 0:
        print ('The numbers you selected are even.')
    else:
        print ('Your numbers are not even. That means they are odd!')
