import random

#create a string of all possible password combos
ch = "abcdefghijklmnopqrstuvwxyz01234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ!@#$%^&*()?"

#make length value
pwlen = 8

#create password by joining a random sample of the string at a certain length
pw = "".join(random.sample(ch,pwlen))

#print the randomly generated password
print(pw)
